<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient android:centerColor="#c9ead1" android:endColor="#c4e9cf" android:startColor="#daede2" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:centerColor="#c9d3ea" android:endColor="#aebce6" android:startColor="#d6e5ea" />
        </shape>
    </item>
</selector>